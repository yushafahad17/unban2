{
  "version": 2,
  "builds": [
    {
      "src": "pages/api/unbanwa.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**/*.{js,ts,jsx,tsx}",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/pages/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
